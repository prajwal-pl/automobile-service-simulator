datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Bike {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  price       Int
  owner       String
  location    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
